{
    "extends": "eslint-config-airbnb",
    "rules": {
        "no-console": 0,
        "eol-last": 0
    },
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true
    },
    "globals": {
        "React": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}