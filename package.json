{
  "name": "codespec",
  "version": "0.0.1",
  "description": "code specification study",
  "main": "",
  "scripts": {
    "test:es6": "babel test/jsx -d test/build/js",
    "watch:demo": "sass --check --trace --sourcemap=none -t expanded -I demo/sass --watch demo/sass:demo/media/css",
    "watch:sass": "sass --check --trace --sourcemap=none -t expanded -I src/sass --watch src/sass:src/media/css",
    "test:sass": "sass --check --trace --sourcemap=none -t expanded -I test/sass/import --update test/sass:test/build/css"
  },
  "keywords": [
    "css specification",
    "javascript specification"
  ],
  "author": "jjpan",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babelify": "^7.3.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
